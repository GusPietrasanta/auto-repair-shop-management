@page "/WatchReport"

@if (@sessionData.GetSearchDetails() != null)
{
	<PageTitle>Report For @sessionData.GetSearchDetails().NumberPlate On @report.TimeStarted.ToString("dd/MM/yy")</PageTitle>
}
else
{
	<PageTitle>Past Reports</PageTitle>
}


@inject ICustomerSessionData sessionData
@inject NavigationManager NavigationManager

<div class="container text-center">
	<h2 class="display-5">Watch Report</h2>

	<button class="btn btn-primary" @onclick="(() => previousPage())">Go to previous page</button>

	

